							<div class="card-header">
								<h3 class="card-title">DataTable of Place to visite</h3>
							</div>
							<!-- /.card-header -->
							<div class="card-body">
								<table id="example2" class="table table-bordered table-hover">
        <tbody>
            <tr>
                <th>IdUser</th>
                <td>{{ user.idUser }}</td>
            </tr>
            <tr>
                <th>UserFirstname</th>
                <td>{{ user.userFirstname }}</td>
            </tr>
            <tr>
                <th>UserLastname</th>
                <td>{{ user.userLastname }}</td>
            </tr>
            <tr>
                <th>UserMail</th>
