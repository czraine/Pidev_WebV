            </tr>
            <tr>
                <th>UserPhone</th>
                <td>{{ user.userPhone }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ user.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user.password }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ user.role }}</td>
            </tr>
            <tr>
                <th>Lang1</th>
                <td>{{ user.lang1 }}</td>
            </tr>
            <tr>
                <th>Lang2</th>
